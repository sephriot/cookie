#: include selectablelabel.kv
#: include listviewlayout.kv

<ListView>:
    viewclass: 'SelectableLabel'
    ListViewLayout:
        orientation: 'vertical'
        padding: [5, 5, 5, 5]
        spacing: 5
        size_hint_y: None
        height: self.minimum_height
        default_size_hint: 1, None
        default_size: None, 48
        multiselect: False